{
    "name": "mbcex",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "node node_modules/vite/bin/vite.js",
        "build": "tsc -b && node node_modules/vite/bin/vite.js build",
        "lint": "node node_modules/eslint/bin/eslint.js .",
        "preview": "node node_modules/vite/bin/vite.js preview",
        "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,css,json}\"",
        "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,css,json}\"",
        "generate:types": "npx openapi-typescript https://api.coingecko.com/api-docs -o src/types/coingecko.ts",
        "deploy": "node node_modules/gh-pages/bin/gh-pages.js -d dist"
    },
    "dependencies": {
        "@lottiefiles/dotlottie-react": "^0.17.5",
        "react": "^19.1.1",
        "react-dom": "^19.1.1",
        "zod": "^4.1.12"
    },
    "devDependencies": {
        "@eslint/js": "^9.36.0",
        "@types/node": "^24.6.0",
        "@types/react": "^19.1.16",
        "@types/react-dom": "^19.1.9",
        "@vitejs/plugin-react": "^5.0.4",
        "babel-plugin-react-compiler": "^19.1.0-rc.3",
        "eslint": "^9.36.0",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-react-refresh": "^0.4.22",
        "gh-pages": "^6.3.0",
        "globals": "^16.4.0",
        "openapi-typescript": "^7.10.1",
        "prettier": "^3.6.2",
        "swagger-typescript-api": "^13.2.16",
        "typescript": "~5.9.3",
        "typescript-eslint": "^8.45.0",
        "vite": "^7.1.7"
    }
}
